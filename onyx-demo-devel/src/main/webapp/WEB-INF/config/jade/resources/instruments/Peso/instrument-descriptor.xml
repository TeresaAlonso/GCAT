  <instrumentParameters>
    <output>
      <code>1_Peso</code>
      <measurementUnit>kg</measurementUnit>
      <dataType>DECIMAL</dataType>
      <captureMethod>MANUAL</captureMethod>
      <integrityChecks>
        <rangeCheck>
          
          <decimalMinValueMale>30.0</decimalMinValueMale>
          <decimalMaxValueMale>270.0</decimalMaxValueMale>
          <decimalMinValueFemale>30.0</decimalMinValueFemale>
          <decimalMaxValueFemale>270.0</decimalMaxValueFemale>
        </rangeCheck>
      </integrityChecks>
    </output>
     <output>
      <code>2_Peso</code>
      <measurementUnit>kg</measurementUnit>
      <dataType>DECIMAL</dataType>
      <captureMethod>MANUAL</captureMethod>
      <integrityChecks>
        <rangeCheck>
          
          <decimalMinValueMale>30.0</decimalMinValueMale>
          <decimalMaxValueMale>270.0</decimalMaxValueMale>
          <decimalMinValueFemale>30.0</decimalMinValueFemale>
          <decimalMaxValueFemale>270.0</decimalMaxValueFemale>
          </rangeCheck>
      <parameterSpreadCheck>
          
          <parameterCode>1_Peso</parameterCode>
          <offset>1</offset>
        </parameterSpreadCheck>
      </integrityChecks>
    </output>
    
       
     
    <output>
      <code>CALC_AVG_PESO</code>
      <measurementUnit>kg</measurementUnit>
      <dataType>INTEGER</dataType>
      <captureMethod>COMPUTED</captureMethod>
      <dataSource class="scriptDataSource">
        <valueType>integer</valueType>
        <unit>mmHg</unit>
        <script>
        $('Peso:1_Peso').plus($('Peso:2_Peso')).div(2).type('integer');
        </script>
      </dataSource>
    </output>
  </instrumentParameters>
</instrumentType>
