<participantAttributes>
	<attribute>
		<name>Enrollment ID</name>
		<assignableAtEnrollment>true</assignableAtEnrollment>
		<mandatoryAtEnrollment>true</mandatoryAtEnrollment>
		<mandatoryAtReception>true</mandatoryAtReception>
		<editableAtReception>true</editableAtReception>
		<editableAfterReception>true</editableAfterReception>
		<type>TEXT</type>
	</attribute>
	<attribute>
		<name>Participant ID</name>
		<assignableAtEnrollment>true</assignableAtEnrollment>
		<mandatoryAtEnrollment>true</mandatoryAtEnrollment>
		<mandatoryAtReception>true</mandatoryAtReception>
		<editableAtReception>true</editableAtReception>
		<editableAfterReception>true</editableAfterReception>
		<type>TEXT</type>
        <validators>
            <dataValidator dataType="TEXT">
                <patternValidator>
                    <pattern>
                        <pattern>^[=][E][0][0][2][5]\d{10}$</pattern>
                    </pattern>
                </patternValidator>
            </dataValidator>
        </validators>
	</attribute>
    	<attribute>
    		<name>First Name</name>
    		<assignableAtEnrollment>true</assignableAtEnrollment>
    		<mandatoryAtEnrollment>true</mandatoryAtEnrollment>
    		<mandatoryAtReception>true</mandatoryAtReception>
    		<editableAtReception>true</editableAtReception>
    		<editableAfterReception>false</editableAfterReception>
    		<type>TEXT</type>
    		<allowedValues>
    			<string>ALTHAIA</string>
    			<string>ARNAU DE VILANOVA</string>
    			<string>BELLVITGE</string>
    			<string>CLINIC</string>
    			<string>GTiP</string>
    			<string>JOAN XXIII</string>
    			<string>JOSEP TRUETA</string>
    			<string>SANT JOAN</string>
    			<string>SANT PAU</string>
    			<string>TERRASSA</string>
    			<string>VALL HEBRON</string>
    			<string>VERGE DE LA CINTA</string>
    		</allowedValues>
    	</attribute>
        <attribute>
            <name>Last Name</name>
            <assignableAtEnrollment>false</assignableAtEnrollment>
            <mandatoryAtEnrollment>false</mandatoryAtEnrollment>
            <mandatoryAtReception>false</mandatoryAtReception>
            <editableAtReception>false</editableAtReception>
            <editableAfterReception>false</editableAfterReception>
            <type>TEXT</type>
            <defaultValues>
                <attributeValue type="TEXT">GCAT</attributeValue>
            </defaultValues>
        </attribute>
	<attribute>
		<name>Birth Date</name>
		<assignableAtEnrollment>true</assignableAtEnrollment>
		<mandatoryAtEnrollment>false</mandatoryAtEnrollment>
		<mandatoryAtReception>true</mandatoryAtReception>
		<editableAtReception>true</editableAtReception>
		<editableAfterReception>false</editableAfterReception>
		<type>DATE</type>
		<validators>
			<dataValidator dataType="DATE">
				<rangeValidator>
					<minimum>1948-01-01</minimum>
                    <maximum>1974-12-31</maximum>
				</rangeValidator>
			</dataValidator>
		</validators>
	</attribute>
	<attribute>
		<name>Gender</name>
		<assignableAtEnrollment>true</assignableAtEnrollment>
		<mandatoryAtEnrollment>false</mandatoryAtEnrollment>
		<mandatoryAtReception>true</mandatoryAtReception>
		<editableAtReception>true</editableAtReception>
		<editableAfterReception>false</editableAfterReception>
		<type>TEXT</type>
		<allowedValues>
			<string>FEMALE</string>
			<string>MALE</string>
		</allowedValues>
	</attribute>
	<attribute>
		<name>Assessment Center ID</name>
		<assignableAtEnrollment>true</assignableAtEnrollment>
		<mandatoryAtEnrollment>true</mandatoryAtEnrollment>
		<mandatoryAtReception>true</mandatoryAtReception>
		<editableAtReception>false</editableAtReception>
		<editableAfterReception>false</editableAfterReception>
		<type>TEXT</type>
	</attribute>
	<attribute>
		<name>Appointment Time</name>
		<assignableAtEnrollment>true</assignableAtEnrollment>
		<mandatoryAtEnrollment>true</mandatoryAtEnrollment>
		<mandatoryAtReception>true</mandatoryAtReception>
		<editableAtReception>false</editableAtReception>
		<editableAfterReception>false</editableAfterReception>
		<type>DATE</type>
	</attribute>
</participantAttributes>