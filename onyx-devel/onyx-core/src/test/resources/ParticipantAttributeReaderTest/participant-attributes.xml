<participantAttributes>
  <group name="doctor">
    <attribute>
      <name>Doctor First Name</name>
      <type>TEXT</type>
      <assignableAtEnrollment>false</assignableAtEnrollment>
      <mandatoryAtEnrollment>false</mandatoryAtEnrollment>
      <mandatoryAtReception>false</mandatoryAtReception>
      <editableAtReception>false</editableAtReception>
      <editableAfterReception>false</editableAfterReception>
      <defaultValues>
        <attributeValue type="TEXT">Default Doctor First Name</attributeValue>
      </defaultValues>
    </attribute>
    <attribute>
      <name>Doctor Last Name</name>
      <type>TEXT</type>
      <assignableAtEnrollment>false</assignableAtEnrollment>
      <mandatoryAtEnrollment>false</mandatoryAtEnrollment>
      <mandatoryAtReception>false</mandatoryAtReception>
      <editableAtReception>false</editableAtReception>
      <editableAfterReception>false</editableAfterReception>
    </attribute>
  </group>
  <attribute>
    <name>Special Needs</name>
    <type>TEXT</type>
    <assignableAtEnrollment>false</assignableAtEnrollment>
    <mandatoryAtEnrollment>false</mandatoryAtEnrollment>
    <mandatoryAtReception>false</mandatoryAtReception>
    <editableAtReception>false</editableAtReception>
    <editableAfterReception>false</editableAfterReception>
  </attribute>
  <group name="familyMember">
    <attribute>
      <name>Family Member First Name</name>
      <type>TEXT</type>
      <assignableAtEnrollment>false</assignableAtEnrollment>
      <mandatoryAtEnrollment>false</mandatoryAtEnrollment>
      <mandatoryAtReception>false</mandatoryAtReception>
      <editableAtReception>false</editableAtReception>
      <editableAfterReception>false</editableAfterReception>
    </attribute>
    <attribute>
      <name>Family Member Last Name</name>
      <type>TEXT</type>
      <assignableAtEnrollment>false</assignableAtEnrollment>
      <mandatoryAtEnrollment>false</mandatoryAtEnrollment>
      <mandatoryAtReception>false</mandatoryAtReception>
      <editableAtReception>false</editableAtReception>
      <editableAfterReception>false</editableAfterReception>
    </attribute>
  </group>
  <attribute>
    <name>Marital Status</name>
    <type>TEXT</type>
    <assignableAtEnrollment>false</assignableAtEnrollment>
    <mandatoryAtEnrollment>false</mandatoryAtEnrollment>
    <mandatoryAtReception>false</mandatoryAtReception>
    <editableAtReception>false</editableAtReception>
    <editableAfterReception>false</editableAfterReception>
  </attribute>
  <attribute>
    <name>Birth Date</name>
    <assignableAtEnrollment>true</assignableAtEnrollment>
    <mandatoryAtEnrollment>false</mandatoryAtEnrollment>
    <mandatoryAtReception>true</mandatoryAtReception>
    <editableAtReception>true</editableAtReception>
    <editableAfterReception>true</editableAfterReception>
    <type>DATE</type>
    <validators>
      <dataValidator dataType="DATE">
        <rangeValidator>
          <minimum>1900-01-01</minimum>
        </rangeValidator>
      </dataValidator>
    </validators>
    <defaultValues>
      <attributeValue type="DATE">2010-01-01 00:00:00</attributeValue>
    </defaultValues>
  </attribute>
</participantAttributes>
